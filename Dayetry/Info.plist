        <string>Fonts/Darky/Darky-ExtraBoldItalic.ttf</string>
        <string>Fonts/Darky/Darky-ExtraLight.ttf</string>
        <string>Fonts/Darky/Darky-ExtraLightItalic.ttf</string>
        <string>Fonts/Darky/Darky-Italic.ttf</string>
        <string>Fonts/Darky/Darky-Light.ttf</string>
        <string>Fonts/Darky/Darky-LightItalic.ttf</string>
        <string>Fonts/Darky/Darky-Medium.ttf</string>
